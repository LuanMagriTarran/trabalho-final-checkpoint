{
    "code": [
      "import os",
      "import pandas as pd",
      "import numpy as np",
      "import matplotlib.pyplot as plt",
      "import ast",
      "",
      "# Função para extrair a classe e confiança com maior valor de confiança",
      "def extract_max_class_confidence(class_list_str):",
      "    try:",
      "        class_list = ast.literal_eval(class_list_str)",
      "        max_class = max(class_list, key=lambda x: x[2])",
      "        return max_class[1], max_class[2]  # Retorna classe e confiança",
      "    except Exception:",
      "        return None, None",
      "",
      "# Criar a pasta \"gráficos\" se não existir",
      "if not os.path.exists(\"gráficos\"):",
      "    os.makedirs(\"gráficos\")",
      "",
      "# Carregar o arquivo de dados com o caminho correto",
      "df = pd.read_csv('distorcoes/distorcoes-data.csv', sep=\";\")",
      "",
      "# Extrair classe e confiança originais",
      "df[\"ORIGINAL_CLASS_LABEL\"], df[\"ORIGINAL_CONFIDENCE\"] = zip(*df[\"ORIG_CLASSES\"].apply(extract_max_class_confidence))",
      "",
      "# Ajustar os nomes das distorções conforme o arquivo CSV",
      "distortions = [\"CANNY\", \"ROTATION\", \"BRIGHTNESS\", \"GAUSSIAN\", \"BLUR\"]",
      "for distortion in distortions:",
      "    class_col = f\"{distortion}_CLASSES\"",
      "    df[f\"{distortion}_CLASS_LABEL\"], df[f\"{distortion}_CONFIDENCE\"] = zip(*df[class_col].apply(extract_max_class_confidence))",
      "",
      "# Definir as colunas de distorção para SSIM e confiança",
      "distortion_columns = {",
      "    \"CANNY\": (\"CANNY_SSIM\", \"CANNY_CONFIDENCE\"),",
      "    \"ROTATION\": (\"ROTATION_SSIM\", \"ROTATION_CONFIDENCE\"),",
      "    \"BRIGHTNESS\": (\"BRIGHTNESS_SSIM\", \"BRIGHTNESS_CONFIDENCE\"),",
      "    \"GAUSSIAN\": (\"GAUSSIAN_SSIM\", \"GAUSSIAN_CONFIDENCE\"),",
      "    \"BLUR\": (\"BLUR_SSIM\", \"BLUR_CONFIDENCE\"),",
      "}",
      "",
      "# Comparação: Como a distorção afeta a confiança média na classe",
      "confidence_means = {distortion: df[f\"{distortion}_CONFIDENCE\"].mean() for distortion in distortions}",
      "plt.figure(figsize=(8, 6))",
      "plt.bar(confidence_means.keys(), confidence_means.values(), color=\"skyblue\")",
      "plt.title(\"Como a distorção afeta a confiança na classe\")",
      "plt.xlabel(\"Tipo de Distorção\")",
      "plt.ylabel(\"Confiança Média na Classe\")",
      "plt.show()",
      "plt.savefig(\"gráficos/confiança_por_distorção.png\")",
      "print(\"Gráfico salvo: gráficos/confiança_por_distorção.png\")",
      "",
      "# Comparação: Como a distorção afeta a Similaridade Estrutural (SSIM)",
      "ssim_means = {distortion: df[f\"{distortion}_SSIM\"].mean() for distortion in distortions}",
      "plt.figure(figsize=(8, 6))",
      "plt.bar(ssim_means.keys(), ssim_means.values(), color=\"salmon\")",
      "plt.title(\"Como a distorção afeta a Similaridade Estrutural\")",
      "plt.xlabel(\"Tipo de Distorção\")",
      "plt.ylabel(\"SSIM Médio\")",
      "plt.show()",
      "plt.savefig(\"gráficos/ssim_por_distorção.png\")",
      "print(\"Gráfico salvo: gráficos/ssim_por_distorção.png\")",
      "",
      "# Comparação: Relação entre Similaridade Estrutural (SSIM) e Confiança para cada distorção",
      "colors = [\"red\", \"green\", \"blue\", \"orange\", \"purple\"]",
      "for i, (distortion, (ssim_col, confidence_col)) in enumerate(distortion_columns.items()):",
      "    plt.figure(figsize=(8, 6))",
      "    plt.scatter(df[ssim_col], df[confidence_col], alpha=0.5, color=colors[i])",
      "    plt.xlabel(\"Similaridade Estrutural (SSIM)\")",
      "    plt.ylabel(\"Confiança na Classificação\")",
      "    plt.title(f\"Relação entre SSIM e Confiança - Distorção: {distortion}\")",
      "    plt.show()",
      "    plt.savefig(f\"gráficos/ssim_vs_confiança_{distortion}.png\")",
      "    print(f\"Gráfico salvo: gráficos/ssim_vs_confiança_{distortion}.png\")",
      "",
      "# Comparação: Como diferentes níveis de contraste afetam a confiança na classe (simulação)",
      "contrast_levels = [0.5, 1.0, 1.5, 2.0]",
      "contrast_confidences = []",
      "for level in contrast_levels:",
      "    simulated_confidence = df[\"ORIGINAL_CONFIDENCE\"].mean() * (1 - 0.1 * (level - 1))  # Exemplo de ajuste",
      "    contrast_confidences.append(simulated_confidence)",
      "",
      "plt.figure(figsize=(8, 6))",
      "plt.plot(contrast_levels, contrast_confidences, marker='o', color=\"purple\")",
      "plt.title(\"Como diferentes níveis de contraste afetam a confiança na classe\")",
      "plt.xlabel(\"Nível de Contraste\")",
      "plt.ylabel(\"Confiança Média na Classe\")",
      "plt.show()",
      "plt.savefig(\"gráficos/confiança_por_nivel_de_contraste.png\")",
      "print(\"Gráfico salvo: gráficos/confiança_por_nivel_de_contraste.png\")",
      "",
      "# Análises Adicionais para a distorção Canny",
      "",
      "# 1. Comparação de Confiança: Canny vs. Original",
      "df['CONFIDENCE_DIFF_CANNY_ORIGINAL'] = df['CANNY_CONFIDENCE'] - df['ORIGINAL_CONFIDENCE']",
      "plt.figure(figsize=(8, 6))",
      "plt.hist(df['CONFIDENCE_DIFF_CANNY_ORIGINAL'], bins=20, color=\"lightcoral\", edgecolor=\"black\")",
      "plt.title(\"Diferença de Confiança entre Canny e Original\")",
      "plt.xlabel(\"Diferença de Confiança (Canny - Original)\")",
      "plt.ylabel(\"Frequência\")",
      "plt.show()",
      "plt.savefig(\"gráficos/diferenca_confianca_canny_original.png\")",
      "print(\"Gráfico salvo: gráficos/diferenca_confianca_canny_original.png\")",
      "",
      "# 2. Comparação de SSIM: Canny vs. Outras Distorções",
      "ssim_comparison = {distortion: df[f\"{distortion}_SSIM\"].mean() for distortion in distortions}",
      "plt.figure(figsize=(10, 6))",
      "plt.bar(ssim_comparison.keys(), ssim_comparison.values(), color=\"skyblue\")",
      "plt.title(\"Comparação de SSIM Médio: Canny vs. Outras Distorções\")",
      "plt.xlabel(\"Tipo de Distorção\")",
      "plt.ylabel(\"SSIM Médio\")",
      "plt.show()",
      "plt.savefig(\"gráficos/ssim_comparacao_canny.png\")",
      "print(\"Gráfico salvo: gráficos/ssim_comparacao_canny.png\")",
      "",
      "# 3. Distribuição de Confiança de Canny em Função de SSIM",
      "plt.figure(figsize=(8, 6))",
      "plt.scatter(df[\"CANNY_SSIM\"], df[\"CANNY_CONFIDENCE\"], alpha=0.5, color=\"blue\")",
      "plt.xlabel(\"SSIM (Canny)\")",
      "plt.ylabel(\"Confiança (Canny)\")",
      "plt.title(\"Relação entre SSIM e Confiança - Distorção Canny\")",
      "plt.show()",
      "plt.savefig(\"gráficos/ssim_vs_confianca_canny.png\")",
      "print(\"Gráfico salvo: gráficos/ssim_vs_confianca_canny.png\")"
    ]
  }
  